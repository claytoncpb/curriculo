let jogador = {
    nome: 'Rogéria',
    idade: 33, 
    jogoFavorito: 'It Takes Two',
    jogosZerados: 5,
    objetivoDaSemana: 3

};
console.log (`Jogador ${jogador.nome}, ${jogador.idade} anos, fã de ${jogador.jogoFavorito}, zerou ${jogador.jogosZerados} jogos está semana.`);

if (jogador.jogosZerados >= jogador.objetivoDaSemana)
{console.log (`Parabéns você completou a maratona da semana!`)}

else (jogador.jogosZerados < jogador.objetivoDaSemana)
{console.log (`Continue jogando! Falta pouco para bater a meta.`)};